// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Clients {
  id         Int      @id @default(autoincrement())
  name       String
  phone      String
  email      String?
  created_at DateTime @default(now())

  @@map("clients")
}
